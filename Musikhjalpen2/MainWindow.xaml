<Window x:Class="Musikhjalpen2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musikhjalpen2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="txtboxWishSongNumber" HorizontalAlignment="Left" Height="26" Margin="68,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="108"/>
        <Button Name="btnWish" Content="Önska" HorizontalAlignment="Left" Height="19" Margin="68,108,0,0" VerticalAlignment="Top" Width="75" Click="btnWish_Click"/>
        <Button Name="btnVisaSaldo" Content="Visa Saldo" HorizontalAlignment="Left" Height="22" Margin="68,141,0,0" VerticalAlignment="Top" Width="75" Click="btnVisaSaldo_Click"/>
        <Button Name="btnTextToSpeech" Content="Text till tal" HorizontalAlignment="Left" Height="34" Margin="68,217,0,0" VerticalAlignment="Top" Width="108" Click="btnTextToSpeech_Click"/>
        <Button Name="btnMostPopularSong" Content="Populäraste låten" HorizontalAlignment="Left" Height="38" Margin="207,60,0,0" VerticalAlignment="Top" Width="134" Click="btnMostPopularSong_Click"/>
        <Button Name="btnGetAllArtists" Content="Hämta alla Artister" HorizontalAlignment="Left" Height="46" Margin="249,171,0,0" VerticalAlignment="Top" Width="151" Click="btnGetAllArtists_Click"/>
        <Button Name="btnArtist" Content="Artist" HorizontalAlignment="Left" Height="29" Margin="451,98,0,0" VerticalAlignment="Top" Width="92" Click="btnArtist_Click"/>
        <Button Name="btnMostPopularArtist" Content="Populäraste Artisten" HorizontalAlignment="Left" Height="32" Margin="369,60,0,0" VerticalAlignment="Top" Width="128" Click="btnMostPopularArtist_Click"/>

    </Grid>
</Window>
